   "source": [
    "''''''\n",
    "import re\n",
    "\n",
    "aa = 'xfsdfagdsaga1123e1243'\n",
    "\n",
    "patternStr = '(?P<key1>\\d{4})(.*)(?P<key2>\\d{4})'\n",
    "pattern = re.compile(patternStr)\n",
    "bb = pattern.search(aa)\n",
    "\n",
    "print(bb.groupdict(0))\n",
    "\n",
    "''''''"
